<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="BBRCust">
    <class name="BBRVisit" table="visits">
        <id name="id" column="VIS_ID">
            <generator class="native"/>
        </id>
        <property name="title" column="TITLE" not-null="true" unique="true"/>
        <property name="posId" column="POS_ID" not-null="true"/>
        <property name="posTitle" column="POS_TITLE"/>
        <property name="length" column="LENGTH"/>
        <property name="price" column="PRICE"/>
        <property name="currency" column="CURRENCY"/>
        <property name="status" column="STATUS"/>
    </class>
</hibernate-mapping>
