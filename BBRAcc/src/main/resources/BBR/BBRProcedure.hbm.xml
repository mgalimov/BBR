<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="BBRCust">
    <class name="BBRProcedure" table="procedures">
        <id name="id" column="VIS_ID">
            <generator class="native"/>
        </id>
        <property name="title" column="TITLE" not-null="true" unique="true"/>
        <property name="length" column="LENGTH"/>
        <property name="price" column="PRICE"/>
        <property name="currency" column="CURRENCY"/>
        <property name="status" column="STATUS"/>
        <many-to-one name="pos" column="POS_ID" class="BBRAcc.BBRPoS" lazy="false" fetch="join"/>
    </class>
</hibernate-mapping>
